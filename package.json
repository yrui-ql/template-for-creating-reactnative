{
  "name": "yhvi",
  "description": "创建ReactNative程序的项目模版",
  "version": "1.0.0",
  "scripts": {
    "cz": "git add . && czg",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "start": "node_modules/.bin/react-native start --experimental-debugger",
    "android": "node_modules/.bin/react-native run-android",
    "ios": "node_modules/.bin/react-native run-ios",
    "windows": "node_modules/.bin/react-native run-windows",
    "build": "NODE_ENV=production node_modules/.bin/react-native",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.20.5",
    "expo-router": "^3.4.7",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.9",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/babel-preset": "^0.73.21",
    "@react-native/metro-config": "^0.73.5",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@tsconfig/react-native": "^3.0.3",
    "@types/react": "18.2.55",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitlint": "^18.6.1",
    "cz-git": "^1.8.0",
    "czg": "1.8.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^14.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "typescript": "5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx,json}": [
      "prettier --write",
      "eslint src/* --cache --cache-location '.cache/.eslintcache' --fix"
    ]
  },
  "private": true
}
